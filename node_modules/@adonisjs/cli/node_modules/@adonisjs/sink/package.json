{
    "name": "@adonisjs/sink",
    "version": "2.5.1",
    "description": "Utilities to create AdonisJs boilerplates and custom providers",
    "main": "index.js",
    "files": [
        "src",
        "index.d.ts",
        "index.js"
    ],
    "scripts": {
        "mrm": "mrm --preset=@adonisjs/mrm-preset",
        "pretest": "npm run lint",
        "test": "node japaFile.js",
        "lint": "eslint . --ext=.ts",
        "clean": "del build",
        "compile": "npm run lint && npm run clean && tsc",
        "build": "npm run compile",
        "commit": "git-cz",
        "release": "np",
        "version": "npm run build"
    },
    "keywords": [
        "adonisjs",
        "mrm",
        "scaffold",
        "utils"
    ],
    "author": "virk,adonisjs",
    "license": "MIT",
    "devDependencies": {
        "@adonisjs/application": "^1.3.9",
        "@adonisjs/dev-utils": "^1.4.0",
        "@adonisjs/fold": "^6.3.4",
        "@adonisjs/mrm-preset": "^2.2.4",
        "@types/fs-extra": "^8.1.0",
        "@types/ini": "^1.3.30",
        "@types/marked": "^0.7.3",
        "@types/node": "^13.7.7",
        "@types/yaml": "^1.2.0",
        "commitizen": "^4.0.3",
        "cz-conventional-changelog": "^3.1.0",
        "del-cli": "^3.0.0",
        "doctoc": "^1.4.0",
        "eslint": "^6.8.0",
        "eslint-plugin-adonis": "^1.0.8",
        "husky": "^4.2.3",
        "ini": "^1.3.5",
        "japa": "^3.0.1",
        "mrm": "^2.1.1",
        "np": "^5.2.1",
        "ts-node": "^8.8.1",
        "typescript": "^3.8.3",
        "yaml": "^1.8.3"
    },
    "nyc": {
        "exclude": [
            "test"
        ],
        "extension": [
            ".ts"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@poppinss/colors": "^1.0.3",
        "@poppinss/fancy-logs": "^1.3.4",
        "@poppinss/prompts": "^1.0.7",
        "@poppinss/utils": "^2.1.2",
        "dot": "^1.1.3",
        "fs-extra": "^9.0.0",
        "marked": "^0.8.2",
        "mrm-core": "^4.0.3",
        "open": "^7.0.3"
    },
    "peerDependencies": {
        "@adonisjs/application": "1.x.x"
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adonisjs/sink.git"
    },
    "bugs": {
        "url": "https://github.com/adonisjs/sink/issues"
    },
    "homepage": "https://github.com/adonisjs/sink#readme",
    "publishConfig": {
        "access": "public",
        "tag": "alpha"
    },
    "np": {
        "contents": "build",
        "anyBranch": false
    }
}
